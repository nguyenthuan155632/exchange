<h1>Operator Management</h1>
<p><%= link_to "Add new Operator", new_operator_store_path %></p>
<table class="table table-hover">
  <tr>
    <th>Name</th>
    <th>Phone</th>
    <th>Deposit</th>
    <th colspan="2"></th>
  </tr>
  
  <% @operators.each do |s| %>
    <tr>
      
      <td><%= link_to "Edit", edit_operator_store_path(s.id) %></td>
      <td><%= link_to "Delete", operator_store_path(s.id), method: :delete, data: {confirm: 'Are you sure?'} %></td>
    </tr>
  <% end -%>
</table>