<h1>User Detail</h1>
<table class="table table-hover">
  <tr>
    <th>User Name</th>
    <th>Email</th>
    <th>Registered Date</th>
    <th colspan="2"></th>
    <th>KYC confirmed</th>
    <th></th>
  </tr>
  
  <% @users.each do |u| %>
    <tr>
      <td><%= u.user_name %></td>
      <td><%= u.email %></td>
      <td><%= u.created_at %></td>
      <td><%= link_to "Detail", operator_user_path(u.id) %></td>
      <td><%= link_to "Edit", edit_operator_user_path(u.id) %></td>
      <td><%= link_to "Edit KYC", edit_operator_kyc_path(u.id) %></td>
      <td><%= link_to "Delete", operator_user_path(u.id), method: :delete, data: {confirm: 'Are you sure?'} %></td>
    </tr>
  <% end -%>
</table>
<%= will_paginate @users %>